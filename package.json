{
	"type": "module",
	"name": "pixelpact-homepage",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"svgr": "svgr src/assets/svgs -d src/components/illustrations/generated --icon --ext .jsx",
		"build": "npm run svgr && vite build",
		"preview": "vite preview",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"playwright:install": "npx playwright install",
		"e2e": "playwright test"
	},
	"vitest": {
		"environment": "jsdom",
		"globals": true,
		"setupFiles": [
			"src/setupTests.js"
		]
	},
	"dependencies": {
		"lottie-web": "^5.13.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "^6.14.1"
	},
	"devDependencies": {
		"@svgr/cli": "^6.5.0",
		"@testing-library/jest-dom": "6.0.0",
		"@testing-library/react": "14.0.0",
		"@vitejs/plugin-react": "5.0.0",
		"autoprefixer": "10.4.14",
		"jsdom": "22.1.0",
		"postcss": "8.4.24",
		"tailwindcss": "3.4.8",
		"vite": "5.0.9",
		"vitest": "1.3.0"
	}
}
